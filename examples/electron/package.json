{
  "name": "rxdb-example-electron",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run electron\"",
    "electron": "electron . --enable-logging",
    "server": "pouchdb-server --host 0.0.0.0 -p 10102 -m -d /tmp/pouchdb-server/ -n true"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "babel-runtime": "6.23.0",
    "concurrently": "3.5.0",
    "electron": "1.6.11",
    "electron-tabs": "^0.7.0",
    "electron-window-manager": "1.0.4",
    "melanke-watchjs": "1.3.1",
    "pouchdb-adapter-http": "6.2.0",
    "pouchdb-adapter-websql": "6.2.0",
    "pouchdb-replication": "6.2.0",
    "pouchdb-server": "2.3.7",
    "rxdb": "^5.0.0",
    "rxjs": "5.4.1"
  }
}
